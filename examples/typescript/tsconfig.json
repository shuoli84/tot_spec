{
  "compilerOptions": {
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "esModuleInterop": true,
    "allowImportingTsExtensions": true
  },
  "include": ["*.ts"]
}
